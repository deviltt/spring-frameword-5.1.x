<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="apple" class="com.tt.lookupmethod.Apple" scope="prototype" />
	<bean id="bananna" class="com.tt.lookupmethod.Banana" scope="prototype" />


	<bean id="fruitPlate1" class="com.tt.lookupmethod.FruitPlate">
		<lookup-method name="getFruit" bean="apple"/>
	</bean>

	<bean id="fruitPlate2" class="com.tt.lookupmethod.FruitPlate">
		<lookup-method name="getFruit" bean="bananna"/>
	</bean>
</beans>